{
  "compilerOptions": {
    "baseUrl": "https://github.com/annaet/cypress-tags-example",
    "target": "es2016",
    "types": ["@shelex/cypress-allure-plugin"],
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "module": "commonjs",
    "declaration": true,
    "outDir": "./dist",
    "strict": true,
    "allowJs": true,
    "noEmit": true
  },
  "include": [
    "**/*.ts"
  ]
}
